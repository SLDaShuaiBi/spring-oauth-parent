<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.louis.spring.oauth.server.dao.UserDao">

  <select id="selectByUsername" parameterType="java.lang.String" resultType="com.louis.spring.oauth.server.entity.User">
    SELECT * FROM  user WHERE username=#{username}
  </select>

</mapper>